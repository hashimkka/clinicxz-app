{
    "expo": {
        "name": "ClinicXZ",
        "slug": "clinicxz-app",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "dark",
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#0f172a"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.clinicxz.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#0f172a"
            },
            "package": "com.clinicxz.app"
        },
        "plugins": [
            "expo-router"
        ],
        "scheme": "clinicxz",
        "experiments": {
            "typedRoutes": true
        }
    }
}